	"disable-javascript-jit": {
		"Description": "Disables Just-In-Time (JIT) compiled JavaScript, JIT is a massive security risk, it should be disabled by default, but it can risk approx 5-50% performance loss depending on the website. Also disables WebAssembly on most browsers. For Windows, uses Arbitrary Code Guard (ACG) to prevent renderer processes from violating W^X, and enables the Intel hardware shadow stack to enforce backward-edge CFI",
		"Tags": ["SECURITY", "OPTIONAL"],
		"Option": "Do you want to disable Just-In-Time (JIT) compiled JavaScript? (this can cause performance slowdowns, this cannot be excepted per-site)",
		"Configs": {
			"js-flags": {
				"Type": "Flag",
				"Arguments": ["--jitless"],
				"Negates": ["disable-javascript-optimizers", "disable-javascript-jit-policy", "disable-javascript-optimizers-policy"]
			}
		}
	},
	"disable-javascript-jit-policy": {
		"Description": "",
		"Tags": ["SECURITY", "OPTIONAL"],
		"Option": "Do you want to disable Just-In-Time (JIT) compiled JavaScript via policy? (this can cause somewhat big performance slowdowns, this can be excepted per-site with the policy JavaScriptJitAllowedForSites)",
		"Configs": {
			"DefaultJavaScriptJitSetting": {
				"Type": "Policy",
				"Value": 2
			}
		}
	},
	"disable-javascript-optimizers": {
		"Description": "Disables optimizers for JavaScript, reduces security risk of JIT compilation without fully disabling it",
		"Tags": ["SECURITY", "OPTIONAL"],
		"Option": "Do you want to disable JavaScript Optimizing compilers? (this can cause decent performance slowdowns, this cannot be excepted per-site)",
		"Configs": {
			"js-flags": {
				"Type": "Flag",
				"Arguments": ["--disable-optimizing-compilers"],
				"Negates": ["disable-javascript-optimizers-policy"]
			}
		}
	},
